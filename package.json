{
  "name": "no-more-slack-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"NEXT,SANITY\" -c \"cyan,magenta\" \"npm run dev:next\" \"npm run dev:sanity\"",
    "dev:next": "next dev",
    "dev:sanity": "cd sanity-back-end && npm run dev",
    "build": "npm run build:next && npm run build:sanity",
    "build:next": "next build",
    "build:sanity": "cd sanity-back-end && npm run build",
    "start": "next start",
    "lint": "next lint",
    "sanity:deploy": "cd sanity-back-end && npm run deploy",
    "sanity:cors": "cd sanity-back-end && npm run cors"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@portabletext/react": "^4.0.3",
    "@sanity/image-url": "^1.2.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "next-sanity": "^11.4.2",
    "react": "19.1.0",
    "react-awesome-reveal": "^4.3.1",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/stripe": "^8.0.416",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
